{
  "name": "@vieolo/file-management",
  "version": "0.4.1",
  "description": "Offering different utility functions to manage files",
  "main": "export.js",
  "scripts": {
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(date_utility|country_data|react_validation|@vieolo|d3*|internmap|robust-predicates))\" --env=jsdom --runInBand",
    "test-coverage": "npm run test -- --coverage --watchAll=false",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "e2e-server": "BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "e2e": "npm run e2e-server",
    "cy:run": "npx cypress run --spec 'cypress/e2e/specs/**/*'",
    "build-components": "tsc"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/react18": "^1.0.1",
    "@cypress/webpack-dev-server": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.7.8",
    "@types/papaparse": "^5.2.5",
    "@types/pdfmake": "^0.2.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "cypress": "^10.11.0",
    "nyc": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.62.1",
    "web-vitals": "^3.3.2"
  },
  "dependencies": {
    "exceljs": "^4.3.0",
    "jszip": "^3.10.1",
    "papaparse": "^5.3.0",
    "pdf-lib": "^1.17.1",
    "pdfmake": "0.2.6"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@vieolo))"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/dev_app/",
      "!src/export.ts",
      "!src/index.tsx",
      "!src/**/index.ts",
      "!src/**/*.d.{ts,tsx}",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html",
      "json"
    ]
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "exclude": [
      "src/dev_app/",
      "src/index.tsx",
      "src/**/index.ts",
      "src/reportWebVitals.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vieolo/file-management-js.git"
  },
  "author": "Vieolo",
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/Vieolo/file-management-js/issues"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}